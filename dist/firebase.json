{
  "hosting": {
    "public": "mouseandkeys",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "redirects": [
      {
        "source" : "/",
        "destination" : "/en",
        "type" : 301
      },
      {
        "source" : "/mouse",
        "destination" : "/en/mouse",
        "type" : 301
      },
      {
        "source" : "/mouse2",
        "destination" : "/en/mouse2",
        "type" : 301
      }
    ],
    "rewrites": [
      {
        "source": "/en/**",
        "destination": "/en/index.html"
      },
      {
        "source": "/id/**",
        "destination": "/id/index.html"
      }
    ]
  }
}
